{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-unused-expressions": 2,
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "quotes": [
      2,
      "single"
    ],
    "strict": [
      2,
      "function"
    ],
    "no-unused-vars": 2,
    "no-undef": 2,
    "max-len": [
      2,
      120
    ],
    "quote-props": 0,
    "one-var": 0,
    "no-underscore-dangle": 0,
    "var-on-top": 0
  }
}
